    },
    "recordTimes": {
      "type": "integer"
    },
    "userWallet": {
      "type": "string"
    },
    "pet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::pet.pet"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "toSign",
        "inProgress",
        "complete",
        "termination"
      ]
